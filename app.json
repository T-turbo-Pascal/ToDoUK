    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/new_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cheeeeeburashka.app"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
